#!/opt/homebrew/bin/gnuplot

# set term qt font "Ember,18" size 2480,480

# set terminal svg size 2000,500
# set output "peer1.svg"
set xdata time
set xtics format "%Y-%m-%d %H:%M"
set timefmt "%Y-%m-%dT%H:%M%:SZ"
set datafile separator comma

set style fill transparent solid 0.3 noborder

set linetype 1 linecolor rgb "#343049"
set linetype 2 linecolor rgb "#493A34"
set linetype 3 linecolor rgb "#BA8E69"

set title "Latency comparison"

historical="vf.csv"

plot historical using 7:3